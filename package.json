{
  "name": "illiade",
  "version": "1.0.0",
  "description": "The Ultimate App Visual Builder",
  "main": "dist/main/main.js",
  "scripts": {
    "start": "electron .",
    "tsc": "tsc --watch",
    "sass": "sass src/style/style.sass dist/style/style.css --watch",
    "pug": "pug src/view/view.pug -o dist/view --watch",
    "build": "concurrently -c \"blue,magenta,red\" -n \"TypeScript,Sass,Pug\" \"npm run tsc\" \"npm run sass\" \"npm run pug\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ph0enixKM/Illiade.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ph0enixKM/Illiade/issues"
  },
  "homepage": "https://github.com/Ph0enixKM/Illiade#readme",
  "dependencies": {
    "concurrently": "^5.0.0",
    "electron-pug": "^2.0.0",
    "flowchart-core": "^1.1.4",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.23.3",
    "three": "^0.110.0",
    "typescript": "^3.6.4"
  },
  "devDependencies": {
    "electron": "^7.0.1"
  }
}
