{
  "name": "illiade",
  "version": "1.0.0",
  "description": "The Ultimate App Visual Builder",
  "main": "dist/main/main.js",
  "scripts": {
    "package": "electron-packager . Illiade --platform=linux --arch=x64",
    "start": "electron .",
    "babel": "babel --watch src/ --out-dir dist/",
    "sass": "sass src/style/style.sass dist/style/style.css --watch",
    "pug": "pug src/view/view.pug -o dist/view --watch",
    "build": "concurrently -c \"yellow,magenta,red\" -n \"JavaScript,Sass,Pug\" \"npm run babel\" \"npm run sass\" \"npm run pug\"",
    "rebuild": "electron-rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ph0enixKM/Illiade.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ph0enixKM/Illiade/issues"
  },
  "homepage": "https://github.com/Ph0enixKM/Illiade#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "coffeescript": "^2.4.1",
    "concurrently": "^5.0.0",
    "electron": "^7.1.7",
    "electron-packager": "^14.1.1",
    "electron-pug": "^2.0.0",
    "electron-rebuild": "^1.8.8",
    "flowchart-core": "^1.1.4",
    "fs-extra": "^8.1.0",
    "interactjs": "^1.7.3",
    "monaco-editor": "^0.18.1",
    "monaco-editor-textmate": "^2.2.1",
    "monaco-textmate": "^3.0.1",
    "monaco-themes": "^0.3.3",
    "onigasm": "^2.2.4",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.23.3",
    "three": "^0.110.0",
    "tippy.js": "^5.1.2",
    "typescript": "^3.6.4"
  },
  "devDependencies": {}
}
